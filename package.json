{
  "name": "angular-webpack-material-lazyload-typescript-starter-template",
  "version": "1.0.1",
  "description": "An Angular 1.5 + Angular Material + Typescript + Webpack starter template with code splitting and lazy loading",
  "main": "index.js",
  "scripts": {
    "build": "webpack-dev-server --inline --hot",
    "display-error": "webpack --display-error-details",
    "dist": "rimraf dist && webpack --config webpack.prod.config.js",
    "distLocal": "rimraf dist && webpack --config webpack.prod.config.js",
    "distDev": "rimraf dist && webpack --config webpack.prod.config.js",
    "analyseBuild": "webpack --json > status.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cleanjs": "rimraf src/app/*.js, src/app/**/*.js, src/app/**/**/*.js"
  },
  "author": "Georgekutty Antony",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-material": "^1.1.1",
    "angular-messages": "^1.5.8",
    "angular-ui-router": "^1.0.0-beta.3",
    "moment": "^2.14.1",
    "oclazyload": "^1.0.9",
    "rxjs": "^5.0.0-beta.11"
  },
  "devDependencies": {
    "@types/angular": "^1.5.16",
    "@types/angular-material": "^1.1.39",
    "@types/jquery": "^2.0.32",
    "@types/oclazyload": "0.0.31",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.16.1"
  }
}
